<layout xmlns:android="http://schemas.android.com/apk/res/android">
    <data>
        <variable
            name="detailResult"
            type="nexon.cyphers.app.adapter.MatchingDetailRecycleAdapter" />
    </data>
<androidx.cardview.widget.CardView
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="2dp"
    android:elevation="6dp"
    android:background="@drawable/circle"
    app:cardCornerRadius="4dp">
    <LinearLayout
        android:id="@+id/detail_recycle"
        android:background="@drawable/circle"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:elevation="6dp"
        >

            <TextView
                android:layout_marginTop="6dp"
                android:layout_marginLeft="7dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="13sp"
                android:textStyle="bold"
                android:text="태그 들어갈 자리."
                android:fontFamily="@font/yeonsung"
                android:id="@+id/matching_Detail_TAG"
                />


        <LinearLayout
            android:layout_marginTop="9dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="4dp">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_weight="1.7">
                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/matching_character_detail_image"
                    android:layout_width="75dp"
                    android:layout_height="75dp"
                    android:layout_gravity="center"
                    android:src="@drawable/bibum2" />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:fontFamily="@font/cookiebold"
                    android:textSize="11sp"
                    android:text="{캐릭터({레벨})}"
                    android:id="@+id/matching_character_detail_NameAndLevel"
                    />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:fontFamily="@font/cookiebold"
                    android:textSize="11sp"
                    android:text="닉네임"
                    android:id="@+id/player_detail_nickname"
                    />

            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="3"
                android:orientation="vertical"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    >
                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@+id/matching_detail_position_info"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_gravity="center"
                        android:layout_weight="2"
                        android:src="@drawable/bibum2" />
                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@+id/matching_position_info_detail_attribute1"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:src="@drawable/bibum2" />
                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@+id/matching_position_info_detail_attribute2"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_gravity="center"
                        android:layout_weight="1"

                        android:src="@drawable/bibum2" />
                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@+id/matching_position_info_detail_attribute3"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_weight="1"

                        android:layout_gravity="center"
                        android:src="@drawable/bibum2" />

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    >
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:layout_marginLeft="5dp"
                        android:layout_weight="1"
                        >
                        <TextView
                            android:layout_marginTop="6dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="left"
                            android:textSize="10sp"
                            android:id="@+id/player_detail_deal_point"
                            android:textStyle="bold"
                            android:fontFamily="@font/ssyoung"
                            android:text="입힌 피해량:"/>
                        <TextView
                            android:layout_marginTop="6dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="left"
                            android:textStyle="bold"
                            android:fontFamily="@font/ssyoung"
                            android:textSize="10sp"
                            android:id="@+id/player_detail_damaged_point"
                            android:text="받은 피해량:"/>
                        <TextView
                            android:layout_marginTop="6dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="left"
                            android:textSize="10sp"
                            android:textStyle="bold"
                            android:fontFamily="@font/ssyoung"
                            android:id="@+id/player_detail_killdeathassist"
                            android:text="킬 데스 어시스트"/>
                        <TextView
                            android:layout_marginTop="6dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="10sp"
                            android:gravity="left"
                            android:textStyle="bold"
                            android:fontFamily="@font/ssyoung"
                            android:id="@+id/player_detail_killdeathassistresult"
                            android:text="KDA:"/>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:layout_gravity="center_vertical"
                        android:gravity="center_vertical"
                        android:layout_weight="1">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="left"
                            android:textSize="10sp"
                            android:id="@+id/player_detail_battlepoint"
                            android:fontFamily="@font/ssyoung"
                            android:textStyle="bold"
                            android:text="전투 참여:"/>
                        <TextView
                            android:layout_marginTop="6dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="left"
                            android:textSize="10sp"
                            android:id="@+id/player_detail_sightpoint"
                            android:textStyle="bold"
                            android:fontFamily="@font/ssyoung"
                            android:text="시야점수:"/>


                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>
        <LinearLayout
